<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.data>
    <DbProviderFactories>
      <add name="PostgreSQL Data Provider" invariant="Npgsql"
         description=".Net Framework Data Provider for PostgreSQL"
         type="Npgsql.NpgsqlFactory, Npgsql"/>
    </DbProviderFactories>
  </system.data>
  <connectionStrings>
    <add name="db" connectionString="Server=127.0.0.1;Port=5432;Database=eventstore;User Id=postgres;Password=root;" providerName="Npgsql"/>
  </connectionStrings>
</configuration>